{
  "name": "typescript-bootstrap-app",
  "version": "1.4.1",
  "description": "This repository is a bootstrap for typescript projects",
  "engineStrict": true,
  "engines": {
    "npm": "10.9.2",
    "node": "=22.15.0"
  },
  "main": "dist/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/CondeNast/typescript-bootstrap.git"
  },
  "private": true,
  "keywords": [],
  "license": "ISC",
  "scripts": {
    "build": "make build",
    "clean": "make clean",
    "check-deps": "make depListUpdates",
    "check-unused": "make depCheck",
    "lint": "make lint",
    "lint-commit": "commitlint --from HEAD~1 --to HEAD --verbose",
    "start": "node dist/index",
    "test": "make test",
    "unit:test": "make unitTest",
    "integration:test": "make integrationTest",
    "tdd": "make tdd",
    "semantic-dry-run": "semantic-release --no-ci --dry-run",
    "semantic-release": "semantic-release",
    "postversion": "cp -r package.json .. && cp -r CHANGELOG.md ..",
    "precommit": "make verify"
  },
  "dependencies": {
    "axios": "1.12.2",
    "config": "4.1.1",
    "datadog-metrics": "0.12.1",
    "express": "5.1.0",
    "inversify": "7.10.2",
    "reflect-metadata": "0.2.2",
    "tslog": "4.10.2"
  },
  "devDependencies": {
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.37.0",
    "@jest/types": "30.2.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/exec": "7.1.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "12.0.2",
    "@semantic-release/release-notes-generator": "14.1.0",
    "@types/config": "3.3.5",
    "@types/express": "5.0.3",
    "@types/jest": "30.0.0",
    "@types/node": "24.7.0",
    "@typescript-eslint/eslint-plugin": "8.46.0",
    "commitlint": "20.1.0",
    "conventional-changelog-conventionalcommits": "9.1.0",
    "depcheck": "1.4.7",
    "eslint": "9.37.0",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-functional": "9.0.2",
    "eslint-plugin-jest": "29.0.1",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-sonarjs": "3.0.5",
    "eslint-plugin-unicorn": "61.0.2",
    "expect-more-jest": "5.5.0",
    "globals": "16.4.0",
    "jest": "30.2.0",
    "jest-silent-reporter": "0.6.0",
    "jest-slow-test-reporter": "1.0.0",
    "jest-watch-master": "1.0.0",
    "jiti": "2.6.1",
    "nock": "14.0.10",
    "npm-check-updates": "19.0.0",
    "semantic-release": "24.2.9",
    "ts-jest": "29.4.4",
    "ts-node": "10.9.2",
    "typescript": "5.9.3"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
